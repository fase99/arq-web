---
import Layout from "@layouts/Layout.astro"
import InnerHero from "@components/sections/InnerHero.astro"
import ContactInfo from "@components/sections/ContactInfo.astro"
import Imagen from '@assets/images/home/pexels-tima-miroshnichenko-5439428.jpg';


const heroContent = {
    title: "Contáctanos",
    description: "Contáctanos para más información",
    backgroundImage: Imagen,
    overlayOpacity: 0.3
}
---

<Layout title="Contáctanos" description="Contáctanos para más información">
    <InnerHero content={heroContent} />
    <ContactInfo background="base" padding="base" />
</Layout>